mn":4}},{"source":"css/styles.scss","name":null,"original":{"line":396,"column":2},"generated":{"line":381,"column":4}}],"sources":{"css/styles.scss":"@import url('https://fonts.googleapis.com/css2?family=Anton&display=swap');\n@import url('https://fonts.googleapis.com/css2?family=Lato:wght@300;400&display=swap');\n\n@import \"base\";\n\n\n:root {\n  --brand: #4f00cf;\n  --font-base: 'Lato', sans-serif;\n  --font-heading: 'Anton', sans-serif;\n  --color-bg: #fff;\n}\n\n* {\n  box-sizing: border-box;\n}\n\nbody {\n  font-family: var(--font-base);\n  font-size: 1rem;\n  font-weight: 300;\n  line-height: 1.4;\n}\n\n.title {\n  font-family: var(--font-heading);\n  text-transform: uppercase;\n}\n\n.title--h1 {\n  font-size: calc(25px + (50 - 14) * ((100vw - 300px) / (1600 - 300)));\n}\n\n.title--h2 {\n  font-size: calc(20px + (25 - 14) * ((100vw - 300px) / (1600 - 300)));\n}\n\n\n// Button\n.btn {\n  font-family: var(--font-base);\n  background-color: var(--brand);\n  color: #fff;\n  border: 0;\n  font-size: .9rem;\n  transition: all .3s ease;\n  border-radius: 2px;\n  padding: 12px 32px;\n  text-transform: uppercase;\n  cursor: pointer;\n  &:hover {\n    color: rgba(255,255,255,.7)\n  }\n  &[disabled] {\n    opacity: .2;\n    cursor: default;\n    color: #fff;\n  }\n  &--copied {\n    position: relative;\n    padding-right: 2.7rem;\n    &:after {\n      content: ' ';\n      position: absolute;\n      top: 50%;\n      right: 1.5rem;\n      display: table;\n      width: 5.71428571px;\n      height: 9.14285714px;\n      border: 2px solid #fff;\n      transform: rotate(45deg) scale(0) translate(-50%,-50%);\n      transition: all .1s cubic-bezier(.71,-.46,.88,.6),opacity .1s;\n\n      border-top: 0;\n      border-left: 0;\n      transform: rotate(45deg) scale(1) translate(-50%,-50%);\n      opacity: 1;\n      transition: all .2s cubic-bezier(.12,.4,.29,1.46) .1s;\n    }\n  }\n}\n\n.page {\n  max-width: 1000px;\n  margin: 0 auto;\n  padding: 0 1rem;\n}\n\n// Header\n.page__header {\n  display: flex;\n  justify-content: center;\n  position: sticky;\n  background-color: rgba(255,255,255,.9);\n  top: 0;\n  z-index: 1;\n}\n.logo {\n  max-width: 100px;\n  display: inline-flex;\n  svg {\n    width: 100%;\n    height: 50px;\n  }\n}\n\n// Checkbox\n\ninput[type=\"checkbox\"] {\n  position: absolute;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  z-index: 1;\n  width: 100%;\n  height: 100%;\n  cursor: pointer;\n  opacity: 0;\n}\n\n.checkbox {\n  background-color: #e9e9e9;\n  width: 22px;\n  height: 22px;\n  display: block;\n  position: absolute;\n  border-radius: 50%;\n  top: .7rem;\n  right: .7rem;\n  opacity: 0;\n  transition: opacity .3s ease;\n  &:after {\n    content: ' ';\n    position: absolute;\n    top: 50%;\n    left: 28%;\n    display: table;\n    width: 5.71428571px;\n    height: 9.14285714px;\n    border: 2px solid #fff;\n    transform: rotate(45deg) scale(0) translate(-50%,-50%);\n    opacity: 1;\n    transition: all .1s cubic-bezier(.71,-.46,.88,.6),opacity .1s;\n  }\n}\n\n// Uses grid\n.users {\n  display: grid;\n  grid-template-columns: var(--users-cols,  repeat(auto-fill, minmax(270px,1fr)));\n  grid-gap: var(--users-gap, 3rem 1.5rem) ;\n  @media (max-width: 600px) {\n    --cols: repeat(auto-fit,minmax(200px,1fr));\n    --users-gap: 2rem 1rem;\n  }\n}\n\n// User component\n.user {\n  position: relative;\n  img {\n    max-width: 100%;\n    width: 100%;\n  }\n  &__image {\n    position: relative;\n  }\n  &__desc {\n    position: absolute;\n    top: 0;\n    right: 0;\n    bottom: 0;\n    left: 0;\n    opacity: 0;\n    display: flex;\n    align-items: center;\n    padding: 2rem;\n    background-color: rgba(255,255,255,.9);\n    transition: opacity .3s ease;\n    &__text {\n      transform: translateY(10px);\n      transition: transform .3s ease-in;\n      opacity: 0;;\n    }\n  }\n  &:hover {\n    .user__desc {\n      cursor: pointer;\n      opacity: 1;\n      &__text {\n        transform: translateY(0);\n        opacity: 1;\n      }\n    }\n    .user__data {\n      cursor: pointer;\n      transition: color .3s ease;\n      color: var(--brand);\n    }\n    .checkbox {\n      opacity: 1;\n    }\n  }\n  &__name {\n    font-size: 21px;\n    margin: 10px 0;\n  }\n  p {\n    margin: 0;\n  }\n}\n\n\ninput[type=\"checkbox\"]:checked {\n  + .user__info .checkbox {\n    background-color: var(--brand);\n    opacity: 1;\n    &:after {\n      border-top: 0;\n      border-left: 0;\n      transform: rotate(45deg) scale(1) translate(-50%,-50%);\n      opacity: 1;\n      transition: all .2s cubic-bezier(.12,.4,.29,1.46) .1s;\n    }\n  }\n}\n\n// FORM\n.form {\n  &__bottom {\n    display: flex;\n    justify-content: center;\n    margin: 2rem 0;\n  }\n}\n\n// Content heading\n.content__header {\n  margin-bottom: var(--content-margin, 2rem);\n  @media (min-width: 601px) {\n    --content-margin: 4rem;\n  }\n  &__info {\n    display: var(--content-grid, grid);\n\n    @media (min-width: 601px) {\n      grid-template-columns: 4fr 10fr;\n    }\n  }\n  .small {\n    text-transform: uppercase;\n    font-weight: bold;\n    font-size: 10px;\n    @media (max-width: 600px) {\n      display: none\n    }\n  }\n  &--selected {\n    display: none;\n  }\n}\n\n.team {\n  display: none;\n  .build__team & {\n    position: fixed;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    background-color: rgba(255,255,255,.95);\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    font-size: 3rem;\n    z-index: 1;\n    padding: 1rem;\n  }\n  &__close {\n    position: absolute;\n    right: 2rem;\n    top: 2rem;\n    width: 30px;\n    height: 30px;\n    cursor: pointer;\n    .line {\n      background-color: #000;\n      top: 14px;\n      left: 7px;\n      position: absolute;\n      width: 18px;\n      height: 2px;\n    }\n    .top {\n      transform: rotate(135deg);\n\n    }\n    .bottom {\n      transform: rotate(45deg);\n    }\n  }\n  &__copy {\n    display: grid;\n    @media (min-width: 601px) {\n      grid-template-columns: 5fr 1fr;\n    }\n    @media (max-width: 600px) {\n      width: 100%;\n    }\n    &__url {\n      background-color: var(--brand);\n      color: #fff;\n      border: 0;\n      font-size: initial;\n      padding: var(--copy-padding, 1rem);\n      @media (min-width: 601px) {\n        border-right: 1px solid rgba(255,255,255,.5);\n        --copy-padding: 1rem 2rem;\n      }\n      @media (max-width: 600px) {\n        border-bottom: 1px solid rgba(255,255,255,.5);\n      }\n    }\n  }\n\n}\n\n.build__team {\n  overflow: hidden;\n}\n\n.leave-note {\n  display: var(--note, none);\n}\n\n.with-team {\n  .content__header--default,\n  .checkbox {\n    display: none;\n  }\n  .content__header--selected {\n    display: block;\n    text-align: center;\n  }\n  .leave-note {\n    --note: grid;\n    grid-gap: 1rem;\n    justify-items: flex-end;\n  }\n}\n\n.textarea {\n  width: 100%;\n  border: 1px solid #eee;\n  min-height: 100px;\n  padding: 1rem;\n  font-size: 1rem;\n}\n\n/* Page Loader */\n.js {\n  .loading {\n    &:before,\n    &:after {\n      content: '';\n      position: fixed;\n      z-index: 1100;\n    }\n    &:before {\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      background: var(--color-bg);\n    }\n    &:after {\n      top: 50%;\n      left: 50%;\n      width: 60px;\n      height: 60px;\n      margin: -30px 0 0 -30px;\n      border-radius: 50%;\n      opacity: 0.4;\n      background: var(--brand);\n      animation: loaderAnim 0.7s linear infinite alternate forwards;\n    }\n  }\n}\n\n\n@keyframes loaderAnim {\n\tto {\n\t\topacity: 1;\n\t\ttransform: scale3d(0.5,0.5,1);\n\t}\n}\n"},"lineCount":null}},"error":null,"hash":"4e8e9f8ab4a44e09af18a6081c60044e","cacheData":{"env":{}}}